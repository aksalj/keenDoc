<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title><%= title || "API Documentation" %></title>


    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.8.0/styles/androidstudio.min.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.8.0/highlight.min.js"></script>

    <!--TODO: Fix slate font-->
    <%- styles %>
    <%- scripts %>

    <% if(language_tabs.length > 0) { %>
    <script>
        $(function() {
            var langs = '<%= language_tabs %>'.split(',');
            setupLanguages(langs);
            hljs.initHighlightingOnLoad();
        });
    </script>
    <% } %>
</head>

<body class="index">
<a href="#" id="nav-button">
      <span>
        NAV
          <img src="img/navbar.png" />
      </span>
</a>
<div class="tocify-wrapper">
    <img src="img/logo.png" />

    <div class="lang-selector">
        <% language_tabs.forEach(function(lang) { %>
            <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
        <% }); %>
    </div>

    <div class="search">
        <input type="text" class="search" id="input-search" placeholder="Search">
    </div>
    <ul class="search-results"></ul>

    <div id="toc"></div>

    <ul class="toc-footer">
        <% toc_footers.forEach(function(toc){ %>
            <li><a href='<%= toc.uri %>'><%= toc.text %></a></li>
        <% });%>
    </ul>

</div>
<div class="page-wrapper">
    <div class="dark-box"></div>

    <div class="content"><%- content %></div>

    <div class="dark-box">
        <div class="lang-selector">
            <% language_tabs.forEach(function(lang) { %>
                <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
            <% }); %>
        </div>
    </div>
</div>
</body>
</html>
